{
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {},
    "impliedStrict": true
  },
  "plugins": [],
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-console": 0,
    "func-names": 0,
    "object-curly-spacing": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-param-reassign": "warn",
    "arrow-parens": 0,
    "semi": ["error", "never"]
  },
  "env": {
    "node": true
  },
  "globals": {}
}
